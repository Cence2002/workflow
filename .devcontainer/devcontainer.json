{
  "name": "Workflow Toolbox",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/home/bence/workflow",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2.5.5": {
      "username": "bence",
      "userUid": "automatic",
      "userGid": "automatic",
      "upgradePackages": false
    }
  },
  "mounts": [
    "source=${localEnv:HOME}/projects,target=/home/bence/projects,type=bind,consistency=cached",
    "source=${localEnv:HOME}/.ssh,target=/home/bence/.ssh,type=bind,consistency=cached"
  ],
  "remoteUser": "bence",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers",
        "googlecloudtools.cloudcode"
      ]
    }
  }
}
